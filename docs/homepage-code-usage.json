  "conditionalRendering": [
    {
      "condition": "coupon.timeLeft exists",
      "component": "CouponRail",
      "description": "Only show time left if coupon has expiry time"
    },
    {
      "condition": "coupon.terms exists",
      "component": "CouponRail",
      "description": "Only show terms if coupon has terms"
    },
    {
      "condition": "coupon.couponType === 'promo_code'",
      "component": "CouponRail",
      "description": "Show different UI for promo codes vs regular coupons"
    },
    {
      "condition": "revealedCodes.has(coupon.id)",
      "component": "CouponRail",
      "description": "Show code only after user clicks reveal button"
    },
